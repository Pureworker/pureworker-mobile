    "androidPush": "appcenter codepush release-react -a pureworkerapp-gmail.com/Pureworker-android -d Staging",
    "androidPush2": "appcenter codepush release-react -a pureworkerapp-gmail.com/Pureworker-android -d Production",
    "codepushall": "yarn iosPush && yarn androidPush",
    "postinstall": "patch-package"
  },
